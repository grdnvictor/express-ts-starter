{
  "name": "express-service",
  "version": "1.0.0",
  "main": "dist/index.js",
  "type": "commonjs",
  "keywords": [],
  "author": "Victor GRANDIN",
  "license": "ISC",
  "description": "",
  "prisma": {
    "schema": "./prisma/schema",
    "seed": "npx tsx prisma/seed.ts"
  },
  "scripts": {
    "_docker:dev": "docker compose --env-file .env.development -f compose.dev.yaml",
    "_docker:prod": "docker compose --env-file .env",
    "dev": "npm run _docker:dev -- up --build",
    "dev:down": "npm run _docker:dev -- down",
    "dev:logs": "npm run _docker:dev -- logs -f server",
    "dev:db": "npm run _docker:dev -- up database -d",
    "dev:clean": "npm run _docker:dev -- down -v",
    "dev:prisma:migrate": "npm run _docker:dev -- exec server npx prisma migrate dev",
    "dev:prisma:studio": "npm run _docker:dev -- up prisma-studio",
    "dev:prisma:reset": "npm run _docker:dev -- exec server npx prisma migrate reset",
    "dev:prisma:generate": "npm run _docker:dev -- exec server npx prisma generate",
    "dev:prisma:seed": "npm run _docker:dev -- exec server npx prisma db seed",
    "build": "tsc && tsc-alias",
    "start": "node dist/index.js",
    "prod": "npm run _docker:prod -- up --build -d",
    "prod:down": "npm run _docker:prod -- down",
    "prod:logs": "npm run _docker:prod -- logs -f server",
    "prod:restart": "npm run _docker:prod -- restart server",
    "prod:clean": "npm run _docker:prod -- down -v",
    "prisma:generate": "npx prisma generate",
    "prisma:migrate": "npx prisma migrate deploy",
    "prisma:studio": "npm run _docker:prod -- --profile tools up prisma-studio",
    "prisma:seed": "npx prisma db seed"
  },
  "dependencies": {
    "@prisma/client": "^6.6.0",
    "@prisma/extension-accelerate": "^1.3.0",
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "jsonwebtoken": "^9.0.2",
    "tsconfig-paths": "^4.2.0",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^22.13.4",
    "fast-glob": "^3.3.2",
    "prisma": "^6.6.0",
    "tsc-alias": "^1.8.10",
    "tsx": "^4.19.2",
    "typescript": "^5.8.3"
  }
}